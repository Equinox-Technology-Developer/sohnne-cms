{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "collections"
    },
    "thumbnail_desktop": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "string",
      "regex": "^\\S*$",
      "required": false
    },
    "description": {
      "type": "text",
      "required": false
    },
    "show_collections": {
      "type": "boolean",
      "default": false
    },
    "meta_title": {
      "type": "string"
    },
    "meta_description": {
      "type": "text"
    },
    "canonical_url": {
      "type": "string"
    },
    "faqs": {
      "displayName": "FAQ",
      "type": "component",
      "repeatable": true,
      "component": "collection.faq"
    },
    "description_collection": {
      "displayName": "description_collection",
      "type": "component",
      "repeatable": true,
      "component": "collection.description-collection"
    }
  }
}
