{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "inversedBy": "products"
    },
    "descriptions": {
      "type": "blocks",
      "required": true
    },
    "features": {
      "type": "blocks",
      "required": true
    },
    "Assembly": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "care_instructions": {
      "type": "blocks",
      "required": true
    },
    "dimension_image_desktop": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "specifications": {
      "type": "component",
      "repeatable": true,
      "component": "table.table",
      "required": true
    },
    "dimension_image_mobile": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "dimensions": {
      "type": "component",
      "repeatable": true,
      "component": "table.table",
      "required": true
    },
    "variants": {
      "type": "component",
      "repeatable": true,
      "component": "product.variant",
      "required": true
    },
    "icon_features": {
      "type": "component",
      "repeatable": true,
      "component": "product.icons"
    },
    "detail_products": {
      "displayName": "Detail Products",
      "type": "component",
      "repeatable": true,
      "component": "product.detail-products"
    },
    "in_the_box": {
      "displayName": "In The Box",
      "type": "component",
      "repeatable": true,
      "component": "product.in-the-box"
    },
    "table_comparison": {
      "type": "dynamiczone",
      "components": [
        "product.comparison-boolean",
        "product.comparison-text"
      ]
    },
    "competitor_name": {
      "displayName": "Competitor Name",
      "type": "component",
      "repeatable": true,
      "component": "product.competitor-name"
    },
    "instagram_feed": {
      "type": "component",
      "repeatable": false,
      "component": "home-page.instagram-feed"
    },
    "product_recommendation": {
      "displayName": "Product Recommendation",
      "type": "component",
      "repeatable": false,
      "component": "product.product-recommendation"
    },
    "quote_card": {
      "displayName": "Quote Card",
      "type": "component",
      "repeatable": false,
      "component": "product.quote-card"
    },
    "estimated_delivery_product": {
      "type": "enumeration",
      "enum": [
        "Within 2-5 Business Days",
        "Within 7-10 Business Days",
        "Within 10-20 Business Days",
        "Within 20-25 Business Days",
        "Within 3-4 Weeks",
        "Within 7-8 Weeks",
        "Within 9-10 Weeks"
      ]
    },
    "badge_text": {
      "type": "string"
    },
    "media_banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product_3d_url": {
      "type": "string"
    },
    "ar_qrcode": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
