{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "inversedBy": "products"
    },
    "descriptions": {
      "type": "blocks",
      "required": false
    },
    "features": {
      "type": "blocks",
      "required": false
    },
    "Assembly": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "care_instructions": {
      "type": "blocks",
      "required": false
    },
    "dimension_image_desktop": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "specifications": {
      "type": "component",
      "repeatable": true,
      "component": "table.table",
      "required": false
    },
    "dimension_image_mobile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "dimensions": {
      "type": "component",
      "repeatable": true,
      "component": "table.table",
      "required": false
    },
    "variants": {
      "type": "component",
      "repeatable": true,
      "component": "product.variant",
      "required": false
    },
    "icon_features": {
      "type": "component",
      "repeatable": true,
      "component": "product.icons"
    },
    "detail_products": {
      "displayName": "Detail Products",
      "type": "component",
      "repeatable": true,
      "component": "product.detail-products"
    },
    "in_the_box": {
      "displayName": "In The Box",
      "type": "component",
      "repeatable": true,
      "component": "product.in-the-box"
    },
    "table_comparison": {
      "type": "dynamiczone",
      "components": [
        "product.comparison-boolean",
        "product.comparison-text"
      ]
    },
    "competitor_name": {
      "displayName": "Competitor Name",
      "type": "component",
      "repeatable": true,
      "component": "product.competitor-name"
    },
    "instagram_feed": {
      "type": "component",
      "repeatable": false,
      "component": "home-page.instagram-feed"
    },
    "product_recommendation": {
      "displayName": "Product Recommendation",
      "type": "component",
      "repeatable": false,
      "component": "product.product-recommendation"
    },
    "quote_card": {
      "displayName": "Quote Card",
      "type": "component",
      "repeatable": false,
      "component": "product.quote-card"
    },
    "estimated_delivery_product": {
      "type": "enumeration",
      "enum": [
        "Within 2-5 Business Days",
        "Within 2-7 Business Days",
        "Within 7-10 Business Days",
        "Within 10-20 Business Days",
        "Within 20-25 Business Days",
        "Within 1-2 Weeks",
        "Within 2-3 Weeks",
        "Within 3-4 Weeks",
        "Within 4-6 Weeks",
        "Within 7-8 Weeks",
        "Within 9-10 Weeks"
      ]
    },
    "badge_text": {
      "type": "string"
    },
    "media_banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ar_qrcode": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "stock_status": {
      "type": "enumeration",
      "enum": [
        "In Stock",
        "Out of Stock"
      ]
    },
    "url_3d_view": {
      "type": "text",
      "maxLength": 10000
    },
    "shipping_classes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::shipping-class.shipping-class",
      "mappedBy": "products"
    },
    "meta_title": {
      "type": "string"
    },
    "meta_description": {
      "type": "text"
    },
    "product_variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::variant.variant"
    },
    "image_list": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "canonical_url": {
      "type": "string"
    },
    "video_reels": {
      "type": "component",
      "repeatable": false,
      "component": "home-page.video-reels"
    },
    "estimated_delivery_product_eu": {
      "type": "enumeration",
      "enum": [
        "Within 2-5 Business Days",
        "Within 2-7 Business Days",
        "Within 7-10 Business Days",
        "Within 10-20 Business Days",
        "Within 20-25 Business Days",
        "Within 1-2 Weeks",
        "Within 2-3 Weeks",
        "Within 3-4 Weeks",
        "Within 4-6 Weeks",
        "Within 7-8 Weeks",
        "Within 9-10 Weeks"
      ]
    },
    "estimated_delivery_product_uk": {
      "type": "enumeration",
      "enum": [
        "Within 2-5 Business Days",
        "Within 2-7 Business Days",
        "Within 7-10 Business Days",
        "Within 10-20 Business Days",
        "Within 20-25 Business Days",
        "Within 1-2 Weeks",
        "Within 2-3 Weeks",
        "Within 3-4 Weeks",
        "Within 4-6 Weeks",
        "Within 7-8 Weeks",
        "Within 9-10 Weeks"
      ]
    },
    "materials": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::material.material",
      "inversedBy": "products"
    },
    "colors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "filter_price": {
      "type": "integer"
    },
    "short_description": {
      "type": "text"
    }
  }
}
