{
  "kind": "collectionType",
  "collectionName": "product_images",
  "info": {
    "singularName": "product-image",
    "pluralName": "product-images",
    "displayName": "Product Image",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "attributes": {
      "type": "enumeration",
      "enum": [
        "Color",
        "Material"
      ],
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "variant"
    },
    "variants": {
      "type": "string",
      "required": true
    }
  }
}
